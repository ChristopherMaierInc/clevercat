<%= form_with model: @skill, local: true, url: "/tutor/skill" do |f| %>


<div class="field">
  <%= f.label :skills %><br>
  <%= f.collection_select(:skill_id, Skill.all, :id, :name) %><br>
</div>

<br>
<div class="actions">
  <%= f.submit class: 'btn btn-primary' %>
</div>

<% end %>
<% @skills.each do |skill| %>
<%= skill.name %><br>

<% end %>

<%= link_to "finished!", profile_path %>
